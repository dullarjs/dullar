{
  "name": "@dullar/earth",
  "version": "0.5.27-alpha.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Advanced and highly reusable Mobile UI Components built on Vue2 & You can define your own UI library by yourself.",
  "author": "just be free <justbefree@126.com>",
  "scripts": {
    "build:components": "node bin/serve && vue-cli-service build --mode production --target lib --name @dullar/earth --dest lib src/index.js",
    "serve": "node bin/serve && vue-cli-service serve --mode development",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "main": "lib/@dullar/earth.umd.min.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib/*.js",
    "types/*.d.ts"
  ],
  "dependencies": {
    "core-js": "^3.8.3",
    "moment": "^2.25.3",
    "vue": "^2.6.14",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-plugin-import": "^1.13.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.2.7",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "svgo-loader": "^3.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/dullarjs/dullar/issues"
  },
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "homepage": "https://github.com/dullarjs/dullar#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "vue ui component",
    "@dullar/earth"
  ],
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dullarjs/dullar.git"
  },
  "gitHead": "0dc2eeb05d14af3b47b1f62d392739eb98bb4e37"
}
